{% extends 'layout.html.twig' %}

{% block title %}Property{% endblock %}

{% block body %}
    <h1>Property</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ property.id }}</td>
            </tr>
            <tr>
                <th>PropertyCategory</th>
                <td>{{ property.propertyCategory }}</td>
            </tr>
            <tr>
                <th>UniqueName</th>
                <td>{{ property.uniqueName }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ property.address }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ property.city }}</td>
            </tr>
            <tr>
                <th>ZipCode</th>
                <td>{{ property.zipCode }}</td>
            </tr>
            <tr>
                <th>Country</th>
                <td>{{ property.country }}</td>
            </tr>
            <tr>
                <th>SurfaceInSquareMeter</th>
                <td>{{ property.surfaceInSquareMeter }}</td>
            </tr>
            <tr>
                <th>NumberOfPiece</th>
                <td>{{ property.numberOfPiece }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ property.description }}</td>
            </tr>
            <tr>
                <th>Equipment</th>
                <td>{{ property.equipment }}</td>
            </tr>
            <tr>
                <th>RentalCategory</th>
                <td>{{ property.rentalCategory }}</td>
            </tr>
            <tr>
                <th>RentExcludingCharges</th>
                <td>{{ property.rentExcludingCharges }}</td>
            </tr>
            <tr>
                <th>Charges</th>
                <td>{{ property.charges }}</td>
            </tr>
            <tr>
                <th>PurchasePrice</th>
                <td>{{ property.purchasePrice }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('property_index') }}">back to list</a>

    <a href="{{ path('property_edit', {'id': property.id}) }}">edit</a>

    {{ include('property/_delete_form.html.twig') }}
{% endblock %}
